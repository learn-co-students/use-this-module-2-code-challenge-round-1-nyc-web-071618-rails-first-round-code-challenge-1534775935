<h2>All Heroines</h2>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
    </li>
    <br>
  <% end %>
</ul>



<%= form_tag("/powers", method: "get") do %>
  <%= label_tag(:powers_path, "Search for powers:") %>
  <%= text_field_tag(:powers_path) %>
  <%= submit_tag("Search") %>
<% end %>
